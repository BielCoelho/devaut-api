{
  "name": "devault-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "scripts": {
    "husky:prepare": "husky install",
    "commit": "git cz && node .husky/push.js",
    "precommit": "git add . && npm run lint && git add .",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:ngrok": "npx ngrok http 4000",
    "prelint": "prettier --write . --loglevel silent",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 --fix",
    "postlint": "tsc --project tsconfig.json --noEmit",
    "run:migration": "prisma migrate dev && prisma generate",
    "run:seeds": "ts-node prisma/seeds"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^4.7.1",
    "apollo-server-express": "^3.11.1",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.2",
    "jest": "^27.2.5",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
